// RUN: %target-sil-opt -module-name Swift -irgen-prepare %s -enable-sil-verify-all | %FileCheck %s

sil_stage raw

import Builtin

// CHECK-LABEL: sil [transparent] [canonical] @polymorphic_builtin_to_trap : $@convention(thin) <T> (@in T, @in T) -> @out T {
// CHECK-NOT: builtin "generic_add"
// CHECK: builtin "int_trap"
// CHECK-NOT: builtin "generic_add"
// CHECK: } // end sil function 'polymorphic_builtin_to_trap'
sil [transparent] [canonical] @polymorphic_builtin_to_trap : $@convention(thin) <T> (@in T, @in T) -> @out T {
bb0(%0 : $*T, %1 : $*T, %2 : $*T):
  %3 = builtin "generic_add"<T>(%0 : $*T, %1 : $*T, %2 : $*T): $()
  %9999 = tuple()
  return %9999 : $()
}

// CHECK-LABEL: sil [transparent] [canonical] @isCallerTransparent : $@convention(thin) () -> Builtin.Int1 {
// CHECK: bb0:
// CHECK-NEXT: [[RESULT:%.*]] = integer_literal $Builtin.Int1, -1
// CHECK-NEXT: return [[RESULT]]
// CHECK-NEXT: } // end sil function 'isCallerTransparent'
sil [transparent] [canonical] @isCallerTransparent : $@convention(thin) () -> Builtin.Int1 {
bb0:
  %1 = builtin "isCallerTransparent"() : $Builtin.Int1
  return %1 : $Builtin.Int1
}

// CHECK-LABEL: sil [canonical] @isCallerNotTransparent : $@convention(thin) () -> Builtin.Int1 {
// CHECK: bb0:
// CHECK-NEXT: [[RESULT:%.*]] = integer_literal $Builtin.Int1, 0
// CHECK-NEXT: return [[RESULT]]
// CHECK-NEXT: } // end sil function 'isCallerNotTransparent'
sil [canonical] @isCallerNotTransparent : $@convention(thin) () -> Builtin.Int1 {
bb0:
  %1 = builtin "isCallerTransparent"() : $Builtin.Int1
  return %1 : $Builtin.Int1
}
